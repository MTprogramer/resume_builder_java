<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout android:orientation="vertical" android:visibility="visible" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:textSize="7dp" android:id="@+id/digree" android:fontFamily="@font/poppins_medium" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/_5sdp" android:text="Degree Name" android:layout_marginStart="@dimen/_10sdp"/>
        <TextView android:textSize="7dp" android:id="@+id/instituteName" android:fontFamily="@font/regular" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/_5sdp" android:text="University Name" android:layout_marginStart="@dimen/_10sdp"/>
        <TextView android:textSize="7dp" android:id="@+id/completeDate" android:fontFamily="@font/poppins_regular" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/_5sdp" android:text="2014" android:layout_marginStart="@dimen/_10sdp"/>
    </LinearLayout>

</LinearLayout>